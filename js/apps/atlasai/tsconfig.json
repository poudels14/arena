{
  "compilerOptions": {
    "noEmit": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "jsxImportSource": "solid-js",
    "jsx": "preserve",
    "allowImportingTsExtensions": true,
    "strict": true,
    "paths": {
      "~/api/*": ["./api/*"]
    }
  }
}
