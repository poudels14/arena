{
  "name": "arena-dashboard",
  "version": "0.0.1",
  "private": true,
  "arena": {
    "env": {
      "ARENA_SSR": "false"
    },
    "javascript": {
      "resolve": {
        "alias": {
          "~": "./src",
          "@arena/core/router": "@arena/runtime/server"
        },
        "dedupe": [
          "solid-js",
          "solid-js/web",
          "@solidjs/meta",
          "@solidjs/router",
          "@arena/solid-store",
          "@arena/solid-dnd",
          "@arena/uikit",
          "@arena/sdk",
          "@kobalte/core",
          "@arena/components",
          "highlight.js"
        ]
      }
    },
    "server": {
      "entry": "./entry-server.tsx",
      "env": {
        "SSR": "true"
      },
      "javascript": {
        "resolve": {
          "conditions": [
            "solid",
            "server",
            "deno",
            "node"
          ],
          "external": [
            "@arena/runtime/postgres",
            "@arena/runtime/server",
            "@arena/runtime/filerouter",
            "@arena/cloud",
            "crypto"
          ]
        }
      }
    },
    "client": {
      "env": {
        "SSR": "false"
      },
      "entry": "./entry-client.tsx",
      "javascript": {
        "resolve": {
          "conditions": [
            "browser",
            "solid",
            "development"
          ]
        }
      }
    }
  },
  "dependencies": {
    "@arena/builtins": "workspace:^",
    "@arena/cloud": "workspace:^",
    "@arena/components": "workspace:^0.1.0",
    "@arena/core": "^0.1.0",
    "@arena/email": "workspace:^",
    "@arena/runtime": "workspace:^",
    "@arena/sdk": "workspace:^",
    "@arena/solid-dnd": "workspace:^",
    "@arena/solid-store": "workspace:^0.1.0",
    "@arena/studio": "workspace:^",
    "@arena/uikit": "workspace:^0.1.0",
    "@arena/widgets": "workspace:^",
    "@blueprintjs/icons": "^4.14.3",
    "@kobalte/core": "^0.9.8",
    "@solidjs/meta": "^0.28.6",
    "@solidjs/router": "^0.8.3",
    "@trpc/client": "^10.21.0",
    "@trpc/server": "^10.21.0",
    "@twind/core": "^1.1.3",
    "@twind/preset-radix-ui": "^1.0.7",
    "@twind/preset-tailwind": "^1.1.4",
    "camelcase": "^7.0.1",
    "cookie": "^0.5.0",
    "debounce": "^1.2.1",
    "drizzle-orm": "^0.26.4",
    "highlight.js": "^11.8.0",
    "ky": "^0.33.3",
    "lodash-es": "^4.17.21",
    "marked": "^9.1.5",
    "ms": "^2.1.3",
    "randomcolor": "^0.6.2",
    "redaxios": "^0.5.1",
    "solid-js": "^1.7.11",
    "style-inject": "^0.3.0",
    "url-join": "^5.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.10.5",
    "typescript": "^4.9.5"
  }
}
