{
  "name": "@arena/workspace",
  "version": "0.0.1",
  "description": "",
  "arena": {
    "javascript": {
      "resolve": {
        "alias": {
          "~": "./src"
        },
        "dedupe": [
          "solid-js",
          "@solidjs/router",
          "@solidjs/meta",
          "@arena/core",
          "@arena/solid-store"
        ]
      }
    },
    "server": {
      "env": {
        "SSR": "true"
      },
      "javascript": {
        "resolve": {
          "alias": {
            "~": "./src"
          },
          "conditions": [
            "solid",
            "server",
            "deno"
          ]
        }
      }
    },
    "client": {
      "javascript": {
        "resolve": {
          "conditions": [
            "solid",
            "browser"
          ]
        }
      }
    }
  },
  "main": "index.tsx",
  "exports": {
    "component": "./src/app/index.tsx",
    "api": "./src/api/index.ts"
  },
  "keywords": [],
  "author": "Sagar Poudel",
  "license": "UNLICENSED",
  "dependencies": {
    "@arena/cloud": "workspace:^",
    "@arena/components": "workspace:^",
    "@arena/core": "workspace:^",
    "@arena/llm": "workspace:^",
    "@arena/runtime": "workspace:^",
    "@arena/sdk": "workspace:^",
    "@arena/solid-store": "workspace:^",
    "@arena/uikit": "workspace:^",
    "@blueprintjs/icons": "^4.14.3",
    "@solidjs/meta": "^0.28.6",
    "@solidjs/router": "^0.8.3",
    "@twind/preset-radix-ui": "^1.0.7",
    "@twind/preset-tailwind": "^1.1.4",
    "highlight.js": "^11.8.0",
    "ky": "^0.33.3",
    "lodash-es": "^4.17.21",
    "marked": "^7.0.1",
    "redaxios": "^0.5.1",
    "solid-js": "^1.7.11",
    "style-inject": "^0.3.0",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.7"
  }
}
