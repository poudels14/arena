{
  "name": "@portal/workspace-desktop",
  "version": "0.1.7",
  "description": "Portal Desktop",
  "type": "module",
  "scripts": {
    "build": "bun run node_modules/@portal/cli/bin/cli.ts app bundle && bun run ./build.mjs"
  },
  "keywords": [],
  "author": {
    "name": "Sagar",
    "email": "poudels14@gmail.com"
  },
  "dependencies": {
    "@arena/runtime": "workspace:^",
    "@kobalte/core": "^0.12.6",
    "@portal-apps/assistant": "workspace:^",
    "@portal-apps/drive": "workspace:^",
    "@portal/cli": "workspace:^",
    "@portal/deploy": "workspace:^",
    "@portal/server-core": "workspace:^",
    "@portal/server-dev": "workspace:^",
    "@portal/solid-dnd": "workspace:^",
    "@portal/solid-query": "workspace:^",
    "@portal/solid-router": "workspace:^",
    "@portal/solid-store": "workspace:^",
    "@portal/solid-ui": "workspace:^",
    "@portal/solidjs": "workspace:^",
    "@portal/workspace": "workspace:^",
    "@portal/workspace-cluster": "workspace:^",
    "@portal/workspace-sdk": "workspace:^",
    "@sentry/browser": "^7.111.0",
    "@solidjs/meta": "^0.29.3",
    "@solidjs/router": "^0.13.0",
    "commander": "^12.0.0",
    "solid-js": "^1.8.15"
  },
  "arena": {
    "server": {
      "entry": "entry-server-dev.tsx"
    },
    "javascript": {
      "resolve": {
        "alias": {
          "~/app": "./app",
          "@portal/server/plugins/resolver": "@arena/runtime/resolver",
          "@portal/server/plugins/transpiler": "@arena/runtime/transpiler"
        },
        "dedupe": [
          "solid-js"
        ],
        "conditions": [
          "solid",
          "server",
          "deno",
          "node"
        ]
      }
    }
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "esbuild": "^0.20.2",
    "tailwindcss": "^3.4.1"
  }
}
